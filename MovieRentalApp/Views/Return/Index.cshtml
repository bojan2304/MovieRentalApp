@model IEnumerable<Movie>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Directors name</th>
            <th>Customer Name</th>
            <th>Return</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Movie movie in Model)
        {
            <tr>
                <td>@movie.Id</td>
                <td>@movie.Title</td>
                <td>@movie.Director.Name</td>
                <td>@movie.Borrower.Name</td>
                <td><a class="btn btn-success" asp-controller="Return" asp-action="ReturnMovie" asp-route-bookId="@movie.Id">Return</a></td>
            </tr>
        }
    </tbody>
</table>